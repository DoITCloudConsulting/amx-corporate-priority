{
    "name": "amx/corporate-priority",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.10 || ^11.0 || ^12.0",
        "amx/salesforce": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Amx\\CorporatePriority\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amx\\CorporatePriority\\Providers\\CorporatePriorityServiceProvider"
            ]
        }
    },
    "archive": {
        "exclude": [
            "/.gitignore",
            "/.vscode",
            "/.idea",
            "/tests",
            "/.gitattributes",
            "composer.json",
            "composer.lock",
            "README.md",
            "eslint.config.js",
            ".prettierignore",
            ".prettierrc"
        ]
    }
}
